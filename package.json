{
    "name": "nest-twitter-example",
    "version": "0.2.0",
    "description": "twitter-like api",
    "author": "forz13",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:prod": "node dist/src/main",
        "lint": "./node_modules/eslint/bin/eslint.js ./src --ext .ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f src/console/ormconfig -d src/migrations -n",
        "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f src/console/ormconfig -d src/migrations -n",
        "migration:run": "ts-node node_modules/typeorm/cli.js migration:run -f src/console/ormconfig",
        "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f src/console/ormconfig"
    },
    "dependencies": {
        "@nestjs/common": "^7.4.2",
        "@nestjs/config": "^0.5.0",
        "@nestjs/core": "^7.4.2",
        "@nestjs/jwt": "^7.1.0",
        "@nestjs/passport": "^7.1.0",
        "@nestjs/platform-express": "^7.4.2",
        "@nestjs/swagger": "^4.6.0",
        "@nestjs/typeorm": "^7.1.1",
        "@typescript-eslint/eslint-plugin-tslint": "^3.9.1",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "commander": "^6.0.0",
        "dotenv": "^8.2.0",
        "faker": "^5.0.0",
        "mysql": "^2.18.1",
        "nestjs-command": "^1.3.0",
        "nestjs-console": "^3.1.1",
        "npm": "^6.14.8",
        "passport": "^0.4.1",
        "passport-http-bearer": "^1.0.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.10",
        "request-context": "^2.0.0",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.2",
        "typeorm": "^0.2.25",
        "typescript": "^4.0.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.5.1",
        "@nestjs/schematics": "^7.1.1",
        "@nestjs/testing": "^7.4.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.7",
        "@types/jest": "^26.0.10",
        "@types/node": "^14.6.0",
        "@types/passport-jwt": "^3.0.3",
        "@types/passport-local": "^1.0.33",
        "@types/supertest": "^2.0.10",
        "@types/yargs": "^15.0.5",
        "@typescript-eslint/eslint-plugin": "^3.10.0",
        "@typescript-eslint/parser": "^3.10.0",
        "eslint": "^7.7.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-import-helpers": "^1.0.2",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.4.2",
        "prettier": "^2.1.0",
        "supertest": "^4.0.2",
        "ts-jest": "^26.2.0",
        "ts-loader": "^8.0.3",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-consistent-codestyle": "^1.16.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-plugin-prettier": "^2.1.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
